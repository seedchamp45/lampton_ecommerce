<div class="page-header"><h1>Upload Photo</h1></div>

<div class="media">

  <% @photos.each do |photo| %>
    <div class="media-left">
      <!-- Renders image -->
  	<%= photo.inspect %>
     
    </div>
    <div class="media-body">
      <h4 class="media-heading"><%= photo.title %></h4>
    </div>
  <% end %>
</div>